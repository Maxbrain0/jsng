{
  "name": "jsng-ghost-theme",
  "version": "0.4.0",
  "description": "blog theme for jsng",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build ./theme-js/entry.js -d ./assets --out-file theme.js --experimental-scope-hoisting",
    "parcel": "parcel watch ./theme-js/entry.js -d ./assets --out-file theme.js"
  },
  "keywords": [
    "blog",
    "theme",
    "ghost",
    "ghost-theme",
    "parcel",
    "starter"
  ],
  "author": {
    "name": "Jacob Goodwin",
    "email": "jacob.goodwin@gmail.com"
  },
  "config": {
    "posts_per_page": 6
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "nodemon": "^1.18.11",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-watch-reload": "0.0.2",
    "postcss-modules": "^1.4.1",
    "sass": "^1.18.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "reframe.js": "^2.2.5"
  },
  "watchreload": {
    "files": [
      "**/*.hbs"
    ]
  }
}
